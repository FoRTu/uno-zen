{{!< default}}

<ol id="posts-list">
  {{#foreach posts}}
  <li>
    <a href="{{url}}" title="link to {{{title}}}">{{{title}}}</a>
    <p class="post description">{{excerpt}}&hellip;</p>
    <div class="post meta">
      <time datetime="{{date format="DD MMM YYYY"}}">{{date format="DD MMM YYYY"}}</time>
      <span class="post tags">{{tags prefix="en " separator=" "}}</span>
    </div>
  </li>
  {{/foreach}}
</ol>

{{pagination}}
